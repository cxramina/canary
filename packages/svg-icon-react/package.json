{
  "name": "@harnessio/svg-icon-react",
  "version": "0.1.0",
  "scripts": {
    "clean": "clean-dist",
    "build:cjs": "tsc && esbuild src/*.{ts,tsx} --allow-overwrite --minify --outdir=dist/cjs --format=cjs",
    "build:esm": "tsc && esbuild src/*.{ts,tsx} --allow-overwrite --minify --outdir=dist --format=esm",
    "build": "npm run build:esm && npm run build:cjs",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "module": "./dist/index.js",
  "main": "./dist/cjs/index.js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/harness/canary.git"
  },
  "author": "Harness Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/harness/canary/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/harness/canary",
  "peerDependencies": {
    "@harnessio/svg-icon": "file:../svg-icon",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@harnessio/svg-icon-cli": "file:../svg-icon-cli",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.23.0",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.3"
  }
}
