{
  "name": "@harnessio/icons-noir",
  "version": "1.0.0",
  "description": "icons-noir icon set",
  "type": "module",
  "module": "./dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "(rm -rf dist > /dev/null 2>&1 || rmdir dist /s /q > nul 2>&1)",
    "tsc": "tsc --emitDeclarationOnly",
    "generate-icons": "svg2icon --iconSetName=noir",
    "build": "pnpm tsc; esbuild src/components/*.{ts,tsx} --allow-overwrite --minify --outdir=dist --analyze --format=esm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "0.19.2",
    "typescript": "^5.1.6",
    "@harnessio/svg-icon-cli": "workspace:*"
  },
  "dependencies": {
    "@harnessio/svg-icon": "workspace:*",
    "@harnessio/svg-icon-react": "workspace:*"
  }
}
