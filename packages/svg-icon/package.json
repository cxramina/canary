{
  "name": "@harnessio/svg-icon",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "module": "./dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "(rm -rf dist > /dev/null 2>&1 || rmdir dist /s /q > nul 2>&1)",
    "build": "tsc; esbuild src/index.ts --bundle --allow-overwrite --minify --sourcemap --outdir=dist --analyze --format=esm",
    "lint": "eslint --ext .ts src",
    "fmt": "prettier --write \"./src/**/*.{ts}\"",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "esbuild": "0.19.2",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  }
}
